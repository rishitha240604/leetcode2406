romans={'I':1,4:'IV',5:'V',9:'IX',10:'X',40:'XL',50:'L',90:'XC',100:'C',400:'CD',500:'D',900:'CM',1000:'M'}
num=int(input())
roman_numeral=()
for value,symbol in romans.items():
  while num>=value:
    roman_numeral=roman_numeral+symbol
    num=num-value
 print(roman_numeral)
 num=list(map(int,input().split())
 for i in range(0,len(num),2):
   val=num.pop(i)
   num.insert(i+1,val)
 print(num)  
    
